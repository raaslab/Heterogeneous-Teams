% makingV_Cluster
% INPUTS
% numOfPoints = how many clusters you want
% numLevels = how many 
% OUTPUTS
%

function [V_Cluster] = makingV_Cluster(numOfPoints, numLevels)


count = 1;
for i = 1:numOfPoints
    for j = 1:numLevels
        V_Cluster(count, :) = i;
        count = count+1;
    end
end

end